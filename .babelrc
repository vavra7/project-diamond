{
	"presets": [["next/babel"]],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@components": "./components",
					"@services": "./services",
					"@fonts": "./fonts",
					"@styles": "./styles"
				}
			}
		],
		[
			"console-source",
			{
				"segments": 2,
				"splitSegment": "/"
			}
		]
	]
}
